environment
  method_pointer 'lps'
  results_output
    results_output_file 'for_h5py'

method
  id_method 'lps'
  list_parameter_study
    list_of_points 0.25 0.75
  model_pointer 'nested_m'
  output silent

model
  id_model 'nested_m'
  nested
  sub_method_pointer 'aleatory'
  identity_response_mapping
  variables_pointer 'epist_v'
  responses_pointer 'epist_r'

variables
  active state
  id_variables 'epist_v'
  continuous_state 1
    descriptors 'x3'
  
responses
  id_responses 'epist_r'
  response_functions 4
  descriptors 'f_mean' 'f_stddev' 'c_mean' 'c_stddev'
  no_gradients
  no_hessians

method
  id_method 'aleatory'
  model_pointer 'sim_m'
  sampling
    seed 1337
    samples 1000
  output verbose

model
  id_model 'sim_m'
  single
  variables_pointer 'sim_v'
  responses_pointer 'sim_r'

variables
  id_variables 'sim_v'
  uniform_uncertain 2
    descriptors 'x1' 'x2'
    lower_bounds 0.0 0.0
    upper_bounds 1.0 1.0
  continuous_state 1
    descriptors 'x3'

responses
  id_responses 'sim_r'
  response_functions 2
   descriptors 'f' 'c'
   no_gradients no_hessians

interface
  direct
   analysis_drivers 'text_book'

   
  
