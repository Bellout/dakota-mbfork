
environment
        tabular_data

method,
       bayes_calibration muq
          metropolis_hastings 
          chain_samples = 1000

variables,
	uniform_uncertain = 1
	  upper_bounds   0.6
	  lower_bounds   0.1

interface,
        direct
	  analysis_driver = 'bayes_linear' # This is a test function based on B. William's linear verification tests.

responses,
 	calibration_terms = 1
        calibration_data_file = 'dakota_bayes_linear.1.dat'
          freeform
          num_experiments = 100
          variance_type = 'scalar'
	no_gradients
	no_hessians

