
environment,
	#graphics
	output_precision = 16

method,
	function_train_uq
	  convergence_tol = 0.000001
	  samples_on_emulator = 10000
#      num_samples_for_construction = 200
      # import_build_points_file = "rosenbrock_samples.dat"
      response_levels = .1 1. 50. 100. 500. 1000.
      variance_based_decomp #interaction_order = 1
#      model_pointer = 'M2'
#      model_param_spec = 'FT'
      model_pointer = 'FT'

model,
     id_model = 'FT'
     surrogate global
        function_train
        kick_rank = 2
        adapt_rank
        max_order = 5
        start_rank = 2
        solver_tolerance = 1e-8
        rounding_tolerance = 1e-8
#       verbosity = 0
        max_rank = 10
      dace_method_pointer = 'SAMPLING'

method
  id_method = 'SAMPLING'
  sampling
    samples = 200
    seed = 531 
    sample_type lhs
    model_pointer = 'SIM'

model,
     single
          interface_pointer = 'I1'
          variables_pointer = 'V1'
          responses_pointer = 'R1'
     id_model = 'SIM'

variables,
    id_variables = 'V1'
	uniform_uncertain = 2
	  lower_bounds = -2. -2.
	  upper_bounds =  2.  2.
	  descriptors  = 'x1' 'x2'

interface,
    id_interface = 'I1'
	direct
	analysis_driver = 'rosenbrock'

responses,
    id_responses = 'R1'
	response_functions = 1
	no_gradients
	no_hessians
