#	optpp_q_newton							#s3,#s9,#s15
#	nlpql										#s4,#s10,#s16
#	npsol										#s5,#s11,#s17
	  max_iterations = 500
	  convergence_tolerance = 1e-5	#s0,#s6
#	  convergence_tolerance = 1e-5	#s3,#s5,#s9,#s11,#s15,#s17
#	  convergence_tolerance = 1e-6	#s1,#s7,#s12,#s13
#	  convergence_tolerance = 1e-7	#s2,#s4,#s8,#s10,#s14,#s16

variables,					
	continuous_design = 2					#s0,#s1,#s2,#s3,#s4,#s5
	initial_point       -1.9,  2.0		#s0,#s1,#s2,#s3,#s4,#s5
	lower_bounds        -2.0, -2.0		#s0,#s1,    #s3,#s4,#s5
	upper_bounds         2.0,  2.0		#s0,#s1,    #s3,#s4,#s5
#
#	continuous_design = 10					#s6,#s7,#s8,#s9,#s10,#s11
#	initial_point  -1.9, 2.0, 8 * 0.0	#s6,#s7,#s8,#s9,#s10,#s11
#	lower_bounds        10 * -2.048		#s6,#s7,    #s9,#s10,#s11
#	upper_bounds        10 *  2.048		#s6,#s7,    #s9,#s10,#s11
#
#	continuous_design = 100					#s12,#s13,#s14,#s15,#s16,#s17
#	initial_point -1.9, 2.0, 98 * 0.0	#s12,#s13,#s14,#s15,#s16,#s17
#	lower_bounds        100 * -2.048		#s12,#s13     ,#s15,#s16,#s17
#	upper_bounds        100 *  2.048		#s12,#s13     ,#s15,#s16,#s17

interface,
	direct
	  analysis_driver = 'extended_rosenbrock'
#	  analysis_driver = 'generalized_rosenbrock'
	  deactivate active_set_vector

responses,
	objective_functions = 1
	analytic_gradients
	no_hessians
