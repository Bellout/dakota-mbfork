#	tabular_data_file = 'dakota_rosenbrock_100d_npsol.dat' 	#s25

method,
	conmin_frcg                      #s0,#s10,#s20
#	dot_frcg									#s1,#s11,#s21
#	optpp_cg									#s2,#s12,#s22
#	optpp_q_newton							#s3,#s13,#s23
#	nlpql										#s4,#s14,#s24
#	npsol										#s5,#s15,#s25
	  max_iterations = 500
	  convergence_tolerance = 1e-5
#	  convergence_tolerance = 1e-7	#s2,#s12,#s22 # opt++cg tighter tolerance

variables,					
	continuous_design = 2					#s0,#s1,#s2,#s3,#s4,#s5
	initial_point       -1.9,  2.0		#s0,#s1,#s2,#s3,#s4,#s5
	lower_bounds        -2.0, -2.0		#s0,#s1,    #s3,#s4,#s5
	upper_bounds         2.0,  2.0		#s0,#s1,    #s3,#s4,#s5
#
#	continuous_design = 10					#s10,#s11,#s12,#s13,#s14,#s15
#	initial_point  -1.9, 2.0, 8 * 0.0	#s10,#s11,#s12,#s13,#s14,#s15
#	lower_bounds        10 * -2.048		#s10,#s11,     #s13,#s14,#s15
#	upper_bounds        10 *  2.048		#s10,#s11,     #s13,#s14,#s15
#
#	continuous_design = 100					#s20,#s21,#s22,#s23,#s24,#s25
#	initial_point -1.9, 2.0, 98 * 0.0	#s20,#s21,#s22,#s23,#s24,#s25
#	lower_bounds        100 * -2.048		#s20,#s21,     #s23,#s24,#s25
#	upper_bounds        100 *  2.048		#s20,#s21,     #s23,#s24,#s25

interface,
	direct
	  analysis_driver = 'extended_rosenbrock'
#	  analysis_driver = 'generalized_rosenbrock'
	  deactivate active_set_vector

responses,
	objective_functions = 1
	analytic_gradients
	no_hessians
