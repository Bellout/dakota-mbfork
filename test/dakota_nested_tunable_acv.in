#@ s10: DakotaConfig=HAVE_NPSOL
#@ s11: DakotaConfig=HAVE_NPSOL
#@ s15: DakotaConfig=HAVE_NPSOL
#@ s16: DakotaConfig=HAVE_NPSOL
#@ s17: DakotaConfig=HAVE_NPSOL

environment
	output_precision = 16
	  method_pointer = 'TUNER'
	  tabular_data freeform			#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s12,#s13,#s14,#s15
	    tabular_data_file = 'ps_mlmc.dat' 		#s0
#	    tabular_data_file = 'ps_cvmc.dat' 		#s1
#	    tabular_data_file = 'ps_mfmc.dat' 		#s2
#	    tabular_data_file = 'ps_acv.dat'  		#s3
#	    tabular_data_file = 'ps_mlmc_proj100.dat' 	#s4
#	    tabular_data_file = 'ps_cvmc_proj100.dat' 	#s5
#	    tabular_data_file = 'ps_mfmc_proj100.dat' 	#s6
#	    tabular_data_file = 'ps_acv_proj100.dat'  	#s7
#	    tabular_data_file = 'ps_mlmc_off5000.dat' 	#s12
#	    tabular_data_file = 'ps_cvmc_off5000.dat' 	#s13
#	    tabular_data_file = 'ps_mfmc_off5000.dat' 	#s14
#	    tabular_data_file = 'ps_acv_off5000.dat'  	#s15

##############
# OUTER LOOP #
##############

method,
	id_method = 'TUNER'
	model_pointer = 'TUNER_M'
        vector_parameter_study			#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s10,#s11,#s12,#s13,#s14,#s15
	  step_vector = .02 0.  num_steps = 50	#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s10,#s11,#s12,#s13,#s14,#s15
#	  step_vector = .01 .01 num_steps = 50		#s8
#	efficient_global				#s9
#	  seed = 1234567				#s9
#	  max_iterations = 50				#s9
#	  convergence_tolerance = 1.e-4
# NPSOL + NPSOL induces method recourse to NIP on inner loop
#	npsol_sqp					#s16
#	optpp_q_newton					#s17
#	  convergence_tolerance = 1.e-8			#s16,#s17
	output silent

model,
	id_model = 'TUNER_M'
	nested
	  variables_pointer  = 'TUNER_V'
	  sub_method_pointer = 'UQ'
	  responses_pointer  = 'TUNER_R'
# Variable mapping: "type 2" from RefMan docs.
     	  primary_variable_mapping 'theta1' 'theta2'
# Response mapping: inner loop returns alg perf: est_var, equiv_cost
# Here we pull the estvar result for inner loop mapping of cost-bar -> estvar
	  primary_response_mapping   = 1. 0.
	  secondary_response_mapping = 0. 1.

variables,
	id_variables = 'TUNER_V'
	continuous_design = 2
	  initial_point  = 0.55 0.5235987755983		#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s10,#s11,#s12,#s13,#s14,#s15
#	  initial_point  = 1.0471975511966 0.5235987755983	#s8
#	  initial_point  = 1.25 	   0.75			#s16,#s17
	  lower_bounds   = 2 * 0.5235987755983
	  upper_bounds   = 2 * 1.5707963267949
	  descriptors    = 'outer_theta1' 'outer_theta2'

responses,
# NOTE: This specifies the TOTAL RESPONSE for the outer loop,
#       which is a combination of nested & interface responses.
	id_responses = 'TUNER_R'
	objective_functions = 1
	nonlinear_inequality_constraints = 1
	nonlinear_inequality_upper_bounds = 1100.
	descriptors = 'avgEstVar' 'equivHFEvals'
	no_gradients					#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s10,#s11,#s12,#s13,#s14,#s15
#	numerical_gradients				#s16,#s17
#	  method_source dakota				#s16,#s17
#	  interval_type central				#s16,#s17
#	  fd_step_size = 1.e-2				#s16,#s17
	no_hessians


##############
# INNER LOOP #
##############

method,
	id_method = 'UQ'
	model_pointer = 'ENSEMBLE'
	multilevel_sampling			#s0,#s4,#s12
#	multifidelity_sampling			#s1,#s2,#s5,#s6,#s13,#s14
#	approximate_control_variate acv_mf sqp	#s3,#s7,#s8,#s9,#s10,#s11,#s15,#s16,#s17
	  max_function_evaluations = 1000
	  pilot_samples = 25	     		#s0,#s1,#s2,#s3,#s8
#	  pilot_samples = 100			#s4,#s5,#s6,#s7,#s9,#s10,#s11
#	  pilot_samples = 250			#s16,#s17
#	  solution_mode pilot_projection	#s4,#s5,#s6,#s7,#s9,#s10,#s11,#s16,#s17
#	  pilot_samples = 5000			#s12,#s13,#s14,#s15
#	  solution_mode offline_pilot		#s12,#s13,#s14,#s15
	  seed = 8674132
	  final_statistics estimator_performance
	output silent

model,
	id_model = 'ENSEMBLE'
	variables_pointer = 'HF_VARS'			#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9
#	variables_pointer = 'ENSEMBLE_VARS'		#s10,#s11,#s16,#s17
	surrogate hierarchical 	       	     		#s0,#s1,#s4,#s5,#s12,#s13
	  ordered_model_fidelities = 'LF2' 'LF1' 'HF'	#s0,#s4,#s12
#	  ordered_model_fidelities = 'LF1' 'HF'		#s1,#s5,#s13
#	surrogate model_ensemble			#s2,#s3,#s6,#s7,#s8,#s9,#s10,#s11,#s14,#s15,#s16,#s17
#	  truth_model = 'HF'				#s2,#s3,#s6,#s7,#s8,#s9,#s10,#s11,#s14,#s15,#s16,#s17
#	  unordered_model_fidelities = 'LF2' 'LF1' 	#s2,#s3,#s6,#s7,#s8,#s9,#s10,#s11,#s14,#s15,#s16,#s17

model,
	id_model = 'LF2' # low-fidelity
	variables_pointer = 'LF2_VARS'
#	interface_pointer = 'LF2_INT
	simulation
#	  solution_level_control = 'ModelForm'
#	  solution_level_cost = 0.01 # w = 10 case
	  cost_recovery_metadata = 'cost_model'

model,
	id_model = 'LF1' # mid-fidelity
	variables_pointer = 'LF1_VARS'
#	interface_pointer = 'LF1_INT'
	simulation
#	  solution_level_control = 'ModelForm'
#	  solution_level_cost = 0.1 # w = 10 case
	  cost_recovery_metadata = 'cost_model'

model,
	id_model = 'HF'
	variables_pointer = 'HF_VARS'
#	interface_pointer = 'HF_INT'
	simulation
#	  solution_level_control = 'ModelForm'
#	  solution_level_cost = 1.
	  cost_recovery_metadata = 'cost_model'

variables,
	id_variables = 'LF2_VARS'
	uniform_uncertain = 2
	  lower_bounds    = 2*-1.
	  upper_bounds    = 2* 1.
     	  descriptors     = 'x' 'y'
	continuous_state  = 5								#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s10,#s12,#s13,#s14,#s15
	  initial_state   = 1.5707963267949 1.0471975511966 0.5235987755983 1. 1.	#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s10,#s12,#s13,#s14,#s15
#	  initial_state   = 1.5707963267949 1.0471975511966 0.5235987755983 2.5 0.55	#s8,#s9
     	  descriptors     = 'theta' 'theta1' 'theta2' 'delta' 'gamma'	    		#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s10,#s12,#s13,#s14,#s15
#	continuous_state  = 3	    	     	      	      				#s11,#s16,#s17
#	  initial_state   = 0.5235987755983 1. 1.	      				#s11,#s16,#s17
#     	  descriptors     = 'theta2' 'delta' 'gamma'	      				#s11,#s16,#s17
	discrete_state_set string = 1
	  initial_state = 'LF2'
	  set_values    = 'LF2'
     	  descriptors   = 'ModelForm'  	  

variables,
	id_variables = 'LF1_VARS'
	uniform_uncertain = 2
	  lower_bounds    = 2*-1.
	  upper_bounds    = 2* 1.
     	  descriptors     = 'x' 'y'
	continuous_state  = 5								#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s10,#s12,#s13,#s14,#s15
	  initial_state   = 1.5707963267949 1.0471975511966 0.5235987755983 1. 1.	#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s10,#s12,#s13,#s14,#s15
#	  initial_state   = 1.5707963267949 1.0471975511966 0.5235987755983 2.5 0.55	#s8,#s9
     	  descriptors     = 'theta' 'theta1' 'theta2' 'delta' 'gamma'	    		#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s10,#s12,#s13,#s14,#s15
#	continuous_state  = 1								#s11,#s16,#s17
#	  initial_state   = 1.0471975511966						#s11,#s16,#s17
#     	  descriptors     = 'theta1'							#s11,#s16,#s17
	discrete_state_set string = 1
	  initial_state = 'LF1'
	  set_values    = 'LF1'
     	  descriptors   = 'ModelForm' 	  

variables,
	id_variables = 'HF_VARS'
	uniform_uncertain = 2
	  lower_bounds    = 2*-1.
	  upper_bounds    = 2* 1.
     	  descriptors     = 'x' 'y'
	continuous_state  = 5								#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s12,#s13,#s14,#s15
	  initial_state   = 1.5707963267949 1.0471975511966 0.5235987755983 1. 1.	#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s12,#s13,#s14,#s15
#	  initial_state   = 1.5707963267949 1.0471975511966 0.5235987755983 2.5 0.55	#s8,#s9
     	  descriptors     = 'theta' 'theta1' 'theta2' 'delta' 'gamma'			#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s12,#s13,#s14,#s15
#	continuous_state  = 1								#s10,#s11,#s16,#s17
#	  initial_state   = 1.5707963267949	      					#s10,#s11,#s16,#s17
#     	  descriptors     = 'theta'		      					#s10,#s11,#s16,#s17
	discrete_state_set string = 1
	  initial_state = 'HF'
	  set_values    = 'HF'
     	  descriptors   = 'ModelForm'     	  

variables,
	id_variables = 'ENSEMBLE_VARS'
	uniform_uncertain = 2
	  lower_bounds    = 2*-1.
	  upper_bounds    = 2* 1.
     	  descriptors     = 'x' 'y'
	continuous_state  = 5
	  initial_state   = 1.5707963267949 1.0471975511966 0.5235987755983 1. 1.
#	  initial_state   = 1.5707963267949 1.0471975511966 0.5235987755983 2.5 0.55
     	  descriptors     = 'theta' 'theta1' 'theta2' 'delta' 'gamma'
	discrete_state_set string = 1
	  initial_state = 'HF'
	  set_values    = 'HF'
     	  descriptors   = 'ModelForm'     	  
     	       	  
interface,
	analysis_driver = 'tunable_model'
   	direct

responses,
	response_functions = 1
	descriptors = 'Q'
	no_gradients
	no_hessians
	metadata 'cost_model'
