#@ s0: TimeoutDelay=120
#@ s1: TimeoutDelay=120
#@ s2: TimeoutDelay=120
#@ s4: TimeoutDelay=120
#@ s5: TimeoutDelay=120
#@ p0: MPIProcs=4
#@ p1: MPIProcs=6

# DAKOTA INPUT FILE : dakota_cantilever_ego.in
# Note: currently this test file performs three different tests:
# (1) efficient_global for constrained, global opt. with mean random vars
# (2) efficient_global for constrained, global NLS  with mean random vars
# s3: test correct traits coverage
# s5: s0 with experimental gaussian process

method,
    efficient_global                        #s0,#s1,#s2,#s3,#s4,#s5,#p0,#p1,
      gaussian_process dakota               #s0,#s4,#p0,#p1
#     gaussian_process surfpack             #s1
#     gaussian_process experimental         #s5
#     convergence_tolerance 1.0e-3          #s3
#     x_conv_tol 1.0e-2                     #s3
#     historical default:
      seed = 123457
#     batch_size = 4                        #s4,#p0
#     batch_size = 6                        #p1

variables,
    continuous_design = 2
      initial_point    4.0          4.0
      upper_bounds    10.0         10.0
      lower_bounds     1.0          1.0
      descriptors     'w' 't'
    continuous_state = 4
      initial_state   40000.  29.E+6  500.  1000.
      descriptors       'R'    'E'    'X'    'Y'

interface,
    direct				    #s0,#s1,#s2,#s3,#s5,#p0,#p1
#   fork asynchronous			    #s4
      analysis_driver = 'cantilever'

responses,
    objective_functions = 1                             #s0,#s1,#s3,#s4,#s5,#p0,#p1
    nonlinear_inequality_constraints = 2                #s0,#s1,#s4,#s5,#p0,#p1
#   nonlinear_equality_constraints = 1                  #s3
#   nonlinear_inequality_constraints = 1                #s3
#   calibration_terms = 3                               #s2
    no_gradients
    no_hessians
